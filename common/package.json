        "command-line-args": "^5.2.1",
        "glob": "^8.1.0",
        "react": "^18.2.0",
        "react-intl": "^6.2.7",
        "tslib": "^2.5.0",
        "typescript-react-intl": "^0.4.1",
        "workbox-webpack-plugin": "^6.5.4"
    },
    "devDependencies": {
        "@swc/jest": "^0.2.24",
        "@testing-library/react": "^13.4.0",
        "@types/command-line-args": "^5.2.0",
        "@types/glob": "^8.0.1",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/webpack-env": "^1.18.0",
