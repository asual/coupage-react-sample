        "glob": "^10.2.7",
        "react": "^18.2.0",
        "react-intl": "^6.4.4",
        "tslib": "^2.5.3",
        "typescript-react-intl": "^0.4.1",
        "workbox-webpack-plugin": "^7.0.0"
    },
    "devDependencies": {
        "@swc/jest": "^0.2.26",
        "@testing-library/react": "^14.0.0",
        "@types/command-line-args": "^5.2.0",
        "@types/glob": "^8.1.0",
        "@types/node": "^20.3.1",
