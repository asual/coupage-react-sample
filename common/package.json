    "private": true,
    "bin": {
        "common": "bin/common"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "rm -rf dist && tspc",
        "intl": "node dist/intl.js",
        "lint": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
        "prepack": "if test -f tsconfig.json; then npm run lint && npm run build && cp -r .npmignore bin intl package.json dist; fi",
        "postpack": "if test -f tsconfig.json; then npm pack ./dist && rm -rf dist/.npmignore dist/bin dist/intl dist/package.json; fi",
        "test": "jest"
    },
    "dependencies": {
        "@coupage/core": "^0.2.6",
        "@coupage/react": "^0.2.6",
